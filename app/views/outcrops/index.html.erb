<p id="notice"><%= notice %></p>

<h1>Outcrops</h1>

<table>
  <thead>
    <tr>
      <th>Uuid</th>
      <th>Altitude</th>
      <th>Date time</th>
      <th>Description</th>
      <th>Horizontal datum</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th>Name</th>
      <th>Toponomy</th>
      <th>Stage uuid</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @outcrops.each do |outcrop| %>
      <tr>
        <td><%= outcrop.uuid %></td>
        <td><%= outcrop.altitude %></td>
        <td><%= outcrop.date_time %></td>
        <td><%= outcrop.description %></td>
        <td><%= outcrop.horizontal_datum %></td>
        <td><%= outcrop.latitude %></td>
        <td><%= outcrop.longitude %></td>
        <td><%= outcrop.name %></td>
        <td><%= outcrop.toponomy %></td>
        <td><%= outcrop.stage_uuid %></td>
        <td><%= link_to 'Show', outcrop %></td>
        <td><%= link_to 'Edit', edit_outcrop_path(outcrop) %></td>
        <td><%= link_to 'Destroy', outcrop, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Outcrop', new_outcrop_path %>
